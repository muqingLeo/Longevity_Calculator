<svg viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
  <!-- Clock/circular background representing time/longevity -->
  <circle cx="50" cy="50" r="45" fill="#e1f5fe" stroke="#3498db" stroke-width="2"/>
  
  <!-- Heart representing health -->
  <path d="M50,75 C50,75 25,60 25,40 C25,30 35,25 45,30 L50,35 L55,30 C65,25 75,30 75,40 C75,60 50,75 50,75 Z" fill="#e74c3c" stroke="#c0392b" stroke-width="1" />
  
  <!-- Clock hands representing biological vs chronological age -->
  <line x1="50" y1="50" x2="50" y2="25" stroke="#2c3e50" stroke-width="3" stroke-linecap="round" />
  <line x1="50" y1="50" x2="70" y2="60" stroke="#27ae60" stroke-width="3" stroke-linecap="round" />
  
  <!-- Center dot -->
  <circle cx="50" cy="50" r="3" fill="#2c3e50" />
</svg>